<tr> 
  <td> # </td>
  <td> <%= user.id %> </td>
  <td> <%= link_to user.name, user_profile_path(user.name, user.id) %> </td>
  <td> <%= user.provider %> </td>
  <td> <%= user.email %> </td>
  <td> <%= user.admin %> </td>
  <td> <%= link_to "Delete User", destroy_user_path(user.name, user.id),
    class: "btn btn-danger", method: :delete, confirm: "Are you sure?" %> </td>
</tr>
