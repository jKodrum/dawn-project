function showOP(newId,total){
		var temId = '';
		for(i=1; i<=total; i=i+1){
			temId = 'job_0'+i;
			$('#' + temId).hide();
			//MM_showHideLayers(temId,'','hide');	
		}
		$('#' + newId).show();
		//MM_showHideLayers(newId,'','show');	
}
var _global_basehref='http://www.104.com.tw/jobbank/custjob/index.php?r=job&j=57514b2e5d4d465d3839416a33353f2316151492b3b4d45282828286d3f682d2c917j55';

function to_buffet_job(){
	if(typeof gaSave === 'function'){	//個人化排序實驗
		gaSave();
	}
	objname=document.jobform;
	objname.action = 'http://login.104.com.tw/login.cfm?channel=104buffet';
	objname.return_url.value='http://www.104.com.tw/jobbank/login/mail_to_self.cfm';
	objname.target='_blank';
	var jobno =objname.jobno.value;
	if (objname.jobno !=null){
		objname.jobno.value=jobno;
	}else{
		objname.action = 'http://login.104.com.tw/login.cfm?channel=104buffet&save_job='+jobno;
	}
	objname.submit();
}
function setcookie(name,value,exp){
    var cookieValue = targetName + "=" + escape(value);
    var exp = new Date();
    var nowPlus30Days = exp.getTime() + (30 * 24 * 60 * 60 * 1000);
    exp.setTime( nowPlus30Days );
    document.cookie = cookieValue + "; expires=" + exp.toGMTString() + ";";
}

//找方向導流 
function redirect_login(cat,flag){
	var url;
	var frm = document.jobform;
	if (flag == 1){
		var func_code = "34qieodkgr";
	}else if (flag == 2){
		var func_code = "35qpdltxfw";
	}
	if (cat){
		frm.return_url.value='http://pda.104.com.tw/forward.cfm?fun_code=' + func_code + '&jobcat='+cat;
		frm.url_destination.value = frm.return_url.value;
	}
	frm.action = 'http://login.104.com.tw/login.cfm';
	frm.target='_blank';
	frm.submit();
}

function mail_to_friend(){
	document.jobform.target='_self';
	document.jobform.action='/jobbank/send_friend/recommendjob.cfm';
	document.jobform.target='_blank';
	document.jobform.submit();
}

function post_jobquest(){
		var item = new Array();
		$("#jobQuest input:checked").each(function() {
			item.push($(this).val());
		});
		if(item != ''){
			var itemJson = '{"checkAry":"'+item+'"}';
			 $.ajax({
	        type:"POST", 
	        url:"/jobbank/custjob/index.php?r=job/suggestion&j=57514b2e5d4d465d3839416a33353f2316151492b3b4d45282828286d3f682d2c917j55",
	        data: {info:itemJson},
	        success: function(data){alert(data);},
	        error:function(){return false;}
		     });
			 $("input:checked").each(function() {
				$(this).prop("checked", false);
	     });
			 MM_showHideLayers('popup_option','','hide');
		}else{
			alert('您尚未勾選原因。');
		}
}

	function gaApply(){		
		GAEvent('jobSorting','Apply', 'N_B');	//個人化排序實驗，點應徵
	}
	function gaSave(){
		GAEvent('jobSorting','Save', 'N_B');	//個人化排序實驗，點儲存
	}
	function gaLoad(){
		GAEvent('jobSorting','Load', 'N_B');	//個人化排序實驗，一Load進頁面觸發
	}
