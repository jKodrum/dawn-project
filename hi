<script type="text/JavaScript">

var strhtml = '';
function jobMyself(objJob,length){
	if(length > 5){
		length = 5;
	}
		if(length > 0 ){		
		strhtml = '<h3>附近親電洽的工作</h3>';
		strhtml = strhtml + '<ul>';
		for (i=0; i<length; i=i+1)
		{
			url = 'http://www.104.com.tw/jobbank/custjob/index.php?r=job&j='+objJob.data[i].J + '&jobsource=volunteer';
			strhtml = strhtml+'<li><a href="'+url+'" onmousedown="adc(2,\'153692\');" target="_blank">'+objJob.data[i].JOB+'</a></li>';
		}
		strhtml += '</ul>';

		$('#jobself').html(strhtml);
		
	}
		if(strhtml == '')
	{
		
		$('#divjobself').remove();
	}	
}
var strhtmls = '';
function jobSimilar(objJob,length,lengths){
		var url='';
	var space='';
	var note='';
	if(lengths > 0){
		$('.sim-job').css('display','block');
		var loop_times = 0;
		if(lengths < 5)loop_times = lengths;
		if(lengths >= 5)loop_times = 5;
		
		for (i=0; i<loop_times; i=i+1) {
			url = 'http://www.104.com.tw/jobbank/custjob/index.php?r=job&j='+objJob.data[i].J + '&jobsource=104_sjob2';
			space = objJob.data[i].JOB_ADDR_NO_DESCRIPT;
			if(((objJob.data[i].PERIOD_DESCRIPT).length==0)&&((objJob.data[i].EDU_DESCRIPT).length==0)){
				note = objJob.data[i].SALARY_DESCRIPT;
			}else{
				note = objJob.data[i].PERIOD_DESCRIPT +'，'+ objJob.data[i].EDU_DESCRIPT+'，'+ objJob.data[i].SALARY_DESCRIPT;
			}
			strhtmls = strhtmls+'<li><a href="'+url+'" onmousedown="adc(2,\'153693\');" target="_blank">'+objJob.data[i].JOB+'</a><p>'+space+'</p><p title="'+note+'">'+note+'</p></li>';
		}
		$('.sim-box ul').append(strhtmls);
		
	}
		if(lengths >= 5){
		$('.sim-box').append('<a href="http://www.104.com.tw/jobbank/joblist/similar_job.cfm?j=57514b2e5d4d465d3839416a33353f2316151492b3b4d45282828286d3f682d2c917j55&jobsource=104_sjob2" target="_blank" onmousedown="adc(2,\'153695\');" class="sim-more" title="104 相似工作頁">看更多相似工作>></a>');
	}
	
	
		if(length > 0){
		if(lengths > 5)lengths = 5;
	}else{
		if(lengths > 10)lengths = 10;
	}
	
	if(lengths > 0 ){	
		strhtmls = '<h3>相似工作&nbsp;';	
				if(lengths >= 5)
		{
			strhtmls += '<a href="http://www.104.com.tw/jobbank/joblist/similar_job.cfm?j=57514b2e5d4d465d3839416a33353f2316151492b3b4d45282828286d3f682d2c917j55&jobsource=104_sjob" target="_blank" onmousedown="adc(2,\'153695\');">更多>></a>';
		}
		strhtmls = strhtmls + '</h3><ul>';
		for (i=0; i<lengths; i=i+1)
		{			
			url = 'http://www.104.com.tw/jobbank/custjob/index.php?r=job&j='+objJob.data[i].J + '&jobsource=104_sjob';
			strhtmls = strhtmls+'<li><a href="'+url+'" onmousedown="adc(2,\'153693\');" target="_blank">'+objJob.data[i].JOB+'</a></li>';
		}
		strhtmls += '</ul>';		
	}	
	
	$('#jobsimilar').html(strhtmls);
	if(strhtmls == '')
	{
		$('#divjobsimilar').remove();
	}
}
function recommendClass(obj,lengths)
{
	var classCount = 5;
	var html = '';
	var url = '';
	if(lengths > classCount)
		lengths = classCount;
	if(lengths > 0)
	{
		html = '<h3>推薦課程</h3><ul>';
		for (var i=0; i<lengths; i++)
		{
			url = 'http://learn.104.com.tw/cfdocs/v2/class.cfm?from=104jobc_5779633_10417880000&theclass='+obj[i].classCode+'&school='+obj[i].schoolCode; 
			html = html + '<li><a href="'+url+'" onmousedown="adc(2,\'153691\');" target="_blank">'+obj[i].className+'</a></li>';
		}
		html = html +'</ul>';
		$('#recommendclass').html(html);
	}
	if(html == '')
	{
		$('#divrecommendclass').remove();
	}
	
}
