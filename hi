	 for(var i=0; i<arr_ad_check11.length; i++) {
		if( $('#aidma_' + arr_ad_check11[i]).is(':empty') ) {
			$("#aidma_" + arr_ad_check11[i]).hide();
		}else{
			_ck11 = 1;
		}
	 }
	 for(var i=0; i<arr_ad_check12.length; i++) {
		if( $('#aidma_' + arr_ad_check12[i]).is(':empty') ) {
			$("#aidma_" + arr_ad_check12[i]).hide();
		}else{
			_ck12 = 1;
		}
	 }
		
	if (_ck11 == 0){$("#right_box11_title").hide();}
	if (_ck12 == 0){$("#right_box12_title").hide();}
	 
</script>
