function login_infomation_post(){
	var frm = document.jobform;
	frm.return_url.value = 'http://www.104.com.tw/jobbank/custjob/index.php?r=job&jobsource=volunteer&j=57514b2e5d4d465d3839416a33353f2316151492b3b4d45282828286d3f682d2c917j55';
	frm.url_destination.value = frm.return_url.value;
	frm.action = 'http://login.104.com.tw/login.cfm';
	frm.submit();
}
function openMap(){
	if (document.getElementById("mapframe") == undefined){
		document.getElementById("map").innerHTML += '<iframe id="mapframe" name="mapframe" src="" height="450px" width="590px" align="center" scrolling="no" topmargin="0" frameborder="0" style="margin-top:5px;"></iframe>';
	}
	document.map_form.submit();
	$('#map').show();
}
function mail_to_company(jobno, op, custno){
	if(typeof gaApply === 'function'){	//個人化排序實驗
		gaApply();
	}
	before_check();
	var op = (op)?0:1;
	var frm = document.jobform;
	frm.jobno.value 	   = jobno;
	frm.cookie_names.value = jobno;
	frm.custno.value = custno;
	if (op)
		frm.target='_blank';
	frm.return_url.value='http://www.104.com.tw/jobbank/login/mail_to_company.cfm?jobsource=volunteer';
	frm.action = 'http://login.104.com.tw/login.cfm?channel=104apply';
	frm.submit();
}
function mail_to_rptapply(_url){
	var frm = document.jobform;
	frm.return_url.value = _url;
	frm.url_destination.value = frm.return_url.value;
	frm.target = 'rpt';
	frm.action = 'http://login.104.com.tw/login.cfm?channel=104rpt';
	frm.submit();
}
function before_check(){
	var login_level = getcookie("LOGIN_LEVEL");
	if(isNaN(login_level) || login_level <= 0) {
		delcookie('TOOLBAR_104_FLAG');
	}
}
function getcookie(name) {
    var thisCookie = document.cookie.split("; ")
    for( i=0; i<thisCookie.length; i++ ) {
        if( name == thisCookie[i].split("=")[0] ) {
            return unescape(thisCookie[i].split("=")[1]);
       }
    }
    return null;
}

function delcookie(name){document.cookie = name + '=;expires=Thu, 10 Jan 1997 00:00:01 GMT;path=/';}
